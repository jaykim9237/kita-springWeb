<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="global.sesoc.web5.dao.ManagerMapper">

<select id="list" resultType="member">
	select
		id
		, password
		, name
	from
		web5_member
	order by
		name asc
</select>
</mapper>